{
    "name": "tailwindcss-addons",
    "description": "A set of Tailwind CSS plugins that add extra defaults, utilities, variants, and more.",
    "version": "1.11.0",
    "license": "MIT",
    "main": "./dist/index.js",
    "repository": "github:babakfp/tailwindcss-addons",
    "homepage": "https://babakfp.ir/docs/tailwindcss-addons",
    "scripts": {
        "dev": "rimraf dist && tsc -w",
        "build": "rimraf dist && tsc",
        "prepublishOnly": "rimraf dist && tsc",
        "format": "prettier -w ."
    },
    "peerDependencies": {
        "tailwindcss": ">= 3.4.1 < 4"
    },
    "devDependencies": {
        "prettier": "3.2.5",
        "rimraf": "6.0.1",
        "tailwindcss": "3.4.3",
        "typescript": "5.4.5"
    },
    "files": [
        "/dist/"
    ],
    "exports": {
        ".": "./dist/index.js"
    },
    "keywords": [
        "ui",
        "css",
        "tailwind",
        "tailwindcss",
        "bg-grid",
        "bg-radial",
        "dir",
        "drag",
        "dynamic-screen",
        "extra-defaults",
        "flex-grid",
        "flip",
        "font-regular",
        "hide-show",
        "hocus",
        "inset-center",
        "not",
        "overflow-unset",
        "reset-number-input",
        "reset-search-input",
        "supports-focus",
        "supports-hocus",
        "supports-hover",
        "tap"
    ],
    "type": "module"
}
